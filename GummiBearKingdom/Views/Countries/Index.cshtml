@model IEnumerable<GummiBearKingdom.Models.Country>

@{
    ViewData["Title"] = "Countries";
}

<h1>Countries of Origin</h1>
<p>@Html.ActionLink("Add", "Create")</p>

<div class="card">
    <div class="card-block">
        @foreach(var country in Model)
        {
            <p>@Html.DisplayFor(model => country.Name, new{@class="card-text"})</p>
        }
    </div>
</div>	